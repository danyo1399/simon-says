{
  "name": "@danyo1399/simon-says",
  "version": "0.1.12",
  "description": "",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "umd:main": "dist/index.umd.js",
  "scripts": {
    "dev": "preact watch",
    "publish:all": "npm run publish:npm && npm run publish:gh",
    "publish:gh": "cross-env GH_BUILD=1 preact build --no-prerender && npm run copyAssets && node scripts/gh-publish.js",
    "publish:npm": "npm run build:lib && npm run copyAssets && npm publish --access public",
    "build:lib": "microbundle build -i src/index.wc.js",
    "copyAssets": "copyfiles assets/* dist && copyfiles assets/* build",
    "lint": "eslint '{src,test}/**/*.js'",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "author": "danyo",
  "license": "MIT",
  "devDependencies": {
    "@types/howler": "^2.2.7",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-preact": "^1.3.0",
    "gh-pages": "^3.2.3",
    "goober": "^2.1.9",
    "howler": "^2.2.3",
    "jest": "^27.3.1",
    "jest-preset-preact": "^4.0.2",
    "localforage": "^1.10.0",
    "microbundle": "^0.14.1",
    "preact": "^10.5.7",
    "preact-cli": "^3.0.5",
    "preact-custom-element": "^4.2.1",
    "preact-habitat": "^3.3.0",
    "preact-render-to-string": "^5.1.12",
    "swr": "^1.3.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "dependencies": {
    "copyfiles": "^2.4.1"
  }
}
